config_version=5

[application]

config/name="Pixel_mafia_protype"
run/main_scene="res://Scenes/Game/open_world.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Globalvar="*res://SRC/Globalvar.gd"
SoundManager="*res://Scenes/Menu/sound_manager.tscn"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"Narrateur": "res://addons/dialogic/Character/Narrateur.dch",
"Vinnie": "res://addons/dialogic/Character/Vinnie.dch",
"player": "res://addons/dialogic/Character/player.dch"
}
directories/dtl_directory={
"Scene01": "res://addons/dialogic/Timeline/Scene01.dtl",
"Scene2": "res://addons/dialogic/Timeline/Scene2.dtl",
"Vinnierng": "res://addons/dialogic/Timeline/Vinnierng.dtl"
}
variables={
"city_name": "",
"player_name": ""
}
layout/style_list=["res://addons/dialogic/Style/3.tres"]
layout/default_style="res://addons/dialogic/Style/3.tres"
glossary/default_case_sensitive=true
text/letter_speed=0.03
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
